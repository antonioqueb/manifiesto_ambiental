              name="Manifiestos"
              sequence="50"
              web_icon="manifiesto_ambiental,static/description/icon.png"/>
    
    <!-- Submenú para Manifiestos Actuales -->
    <menuitem id="menu_manifiesto_ambiental_manifiestos"
              name="Manifiestos Actuales"
